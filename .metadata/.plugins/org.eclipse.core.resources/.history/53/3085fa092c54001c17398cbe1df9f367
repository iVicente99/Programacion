import java.util.Scanner;

/**
 * Lee un numero entre 1 y 10 y muestra en pantalla la tabla de multiplicar
 */

/**
 * @author Iván Vicente y Joaquín Coma
 *
 */
public class Ejercicio2 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Scanner teclado = new Scanner (System.in);
		System.out.println("Introduce un numero para mostrar su tabla de multiplicar");
		//System.out.println("Introduce un numero para mostrar su tabla de multiplicar");//
		int a= teclado.nextInt();
		System.out.println("TABLA DE MULTIPLICAR DE "+a);
		while (a <=10) {
			int multiplicacion= a * 1;
					
		}

	}

}
